<Uintah_specification>
  <Meta>
    <title>almgren mms </title>
  </Meta>

  <SimulationComponent type="arches" />

  <Solver type="hypre" />

  <Time>
    <maxTime>100.</maxTime>
    <initTime>0.0</initTime>
    <delt_min>1e-5</delt_min>
    <delt_max>1e-1</delt_max>
    <delt_init>1e-1</delt_init>
    <timestep_multiplier>0.3</timestep_multiplier>
    <max_Timesteps>50</max_Timesteps>
  </Time>

  <DataArchiver>
    <filebase>almgren-mms.uda</filebase>
    <outputTimestepInterval>1</outputTimestepInterval>
    <outputInitTimestep />
    <save label="x-mom" />
    <save label="y-mom" />
    <save label="z-mom" />
    <save label="uVel" />
    <save label="vVel" />
    <save label="wVel" />
    <save label="gamma" />
    <save label="pressure" />
    <save label="b_press" />
    <checkpoint cycle="2" interval=".3" />
    <compression>gzip</compression>
  </DataArchiver>

  <Grid>
    <Level>
      <Box label="1">
        <lower>[0,0,0]</lower>
        <upper>[1.0,1.0,1.0]</upper>
        <resolution>[24,24,24]</resolution>
        <extraCells>[0,0,0]</extraCells>
        <patches>[1,1,1]</patches>
      </Box>
      <periodic>[1,1,1]</periodic>
    </Level>
    <BoundaryConditions>
    </BoundaryConditions>
  </Grid>

  <PhysicalConstants>
    <gravity>[0,0,0]</gravity>
    <reference_point>[0,0,0]</reference_point>
    <viscosity>0.0002</viscosity>
  </PhysicalConstants>

  <CFD>
    <ARCHES>

      <TimeIntegrator order="1">
        <ExplicitIntegrator order="first" />
      </TimeIntegrator>

      <VarID>
        <var label="uVel" role="uvelocity" />
        <var label="vVel" role="vvelocity" />
        <var label="wVel" role="wvelocity" />
        <var label="gamma" role="total_viscosity" />
        <var label="density" role="density" />
      </VarID>

      <Properties>
        <table label="a_user_generated_label" type="constant"><ConstantProps>
          <temperature>298.</temperature>
          <density>298.</density>
        </ConstantProps>
      </table></Properties>

      <PropertyModelsV2>
        <model label="density" type="constant_property">
          <grid type="CC" />
          <value>1.</value>
        </model>
        <model label="gamma" type="constant_property">
          <grid type="CC" />
          <value>1.e-4</value>
        </model>
      </PropertyModelsV2>

      <KMomentum>
        <convection scheme="central" />
        <inviscid />
        <PressureSolver>
          <Parameters>
            <solver>           cg     </solver>
            <preconditioner>  pfmg    </preconditioner>
            <tolerance>      1.0e-10  </tolerance>
            <maxiterations>    75     </maxiterations>
            <skip>              1     </skip>
            <setupFrequency>    0     </setupFrequency>
            <jump>              0     </jump>
          </Parameters>
        </PressureSolver>
      </KMomentum>

      <Initialization>
        <task task_label="x-mom-init" type="almgren_mms" variable_label="x-mom">
          <variable type="FX" />
          <which_vel>u</which_vel>
          <coordinates x="ucellX" y="gridY" />
        </task>
        <task task_label="y-mom-init" type="almgren_mms" variable_label="y-mom">
          <variable type="FY" />
          <which_vel>v</which_vel>
          <coordinates x="gridX" y="vcellY" />
        </task>
      </Initialization>

      <BoundaryConditions>
        <force_area_calc />
      </BoundaryConditions>

      <KokkosSolver>
      </KokkosSolver>

    </ARCHES>
  </CFD>
</Uintah_specification>